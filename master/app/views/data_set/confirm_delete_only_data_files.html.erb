<h1>Project <%= session[:project] %> <span style="color:red;font-size:medium">Delete only data files</span></h1>
<hr />
<% if @delete_candidates.empty? %>
  <ul>
    <li><span style="color:red;">There is no removable data file in this DataSet</span></li>
  </ul>
<% else %>
  <%= form_tag(:action=>"run_delete_only_data_files", :id=>@data_set.id.to_s) do %>
    <%= submit_tag 'delete', :data=>{:confirm=>"Are you sure you want to delete the data files??"} %>
  <% end %>
  <h3>Data files</h3>
  <ol>
    <% @delete_candidates.each do |file| %>
      <li><%= file %></li>
    <% end %>
  </ol>
<% end %>
